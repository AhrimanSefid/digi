<GridLayout3@GridLayout>:
    canvas.after:
        RoundedRectangle:
            pos: self.pos
            size: self.size
            radius: [(dp(5), dp(5)), (dp(5), dp(5)), (dp(5), dp(5)), (dp(5), dp(5))]   
            source: 'img/App/gl3.jpg'
    Button:
        background_color: 0, 0, 0, 0
        on_press:
            app.root.open_category("H")
    Button:
        background_color: 0, 0, 0, 0
        on_press:
            app.root.open_category("GL")
    Button:
        background_color: 0, 0, 0, 0
        on_press:
            app.root.open_category("SW")
    Button:
        background_color: 0, 0, 0, 0
        on_press:
            app.root.open_category("PI")
    Button:
        background_color: 0, 0, 0, 0
        on_press:
            app.root.open_category("SH")
    Button:
        background_color: 0, 0, 0, 0
        on_press:
            app.root.open_category("R")

