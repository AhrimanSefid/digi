<Off@Screen>:
    name: 'off'
    mgr1: _bot_nav
    BoxLayout:
        orientation: 'vertical'
        GridLayout:
            cols: 2   
            size_hint:(1, 1/13)
            padding: 0, 0, dp(15), 0   
            MDLabel:
                size_hint_x: .9     
                halign: "right"
                text: get_display(arabic_reshaper.reshape('تخفیف های ویژه'))
                #font_name: 'font/XB Niloofar.ttf'#irsans.ttf'
            FloatLayout:
                size_hint_x: .1     
                pos_hint: {'top': .5}
                MDIconButton:
                    icon: "window-close"
                    pos_hint: {"center_x": .95, "top": 1}
                    on_press:
                        app.root.current= 'main'

        Widget:
            size_hint_y: None
            height: dp(1)
            canvas:
                Color:
                    rgba: 128/255, 128/255, 128/255, .1
                Rectangle:
                    pos: dp(20), self.y
                    size: root.width - dp(40), self.height
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: 11/13
            ScrollView:
                effect_cls: 'ScrollEffect'
                bar_width: 0
                StackLayoutOff:

        BottomNavigation:
            id: _bot_nav
            #size_hint:(1, 1/13)
            size_hint_y: None
            height: mm(9)#dp(47)
