<Grouping@Screen>:
    name: 'grouping'
    mgr1: _bot_nav
    BoxLayout:
        orientation: 'vertical'
        spacing: dp(5)
        SearchBar:
            id: _search_bar
            #size_hint:(1, 1/13)
            size_hint_y: None
            height: mm(9)
        ScrollView:
            effect_cls: 'ScrollEffect'
            do_scroll_x: False
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: self.minimum_height  
                padding: dp(10), 0, dp(10), 0
                BoxLayout:
                    orientation:'horizontal'
                    size_hint_y: None
                    height: mm(10)
                    MDTextButton:
                        text: get_display(arabic_reshaper.reshape('مشاهده همه'))
                        #font_name: 'font/XB Niloofar.ttf'#irsans.ttf'
                        font_size: '14sp'
                        theme_text_color: "Custom"
                        text_color: 105/255, 171/255, 185/255, 1
                        halign: "left"
                        valign: 'bottom'
                    MDLabel:
                        text: get_display(arabic_reshaper.reshape('زیبایی و سلامت'))
                        #font_name: 'font/XB Niloofar.ttf'#irsans.ttf'
                        halign: "right"
                        valign: 'bottom'
